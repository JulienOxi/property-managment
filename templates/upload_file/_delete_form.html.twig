<form method="post" action="{{ path('app_upload_file_delete', {'id': upload_file.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ upload_file.id) }}">
    <button class="btn"><span class="material-symbols-outlined text-md text-red-600 hover:text-red-800">delete</span></button>
</form>
