<form method="post" action="{{ path('app_access_control_delete', {'id': access_control.id}) }}" onsubmit="return confirm('Voulez vous vraiment retirer cet utilisateur ?');" class="text-right">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ access_control.id) }}">
    <button class="btn"><span class="material-symbols-outlined text-base text-red-600 hover:text-red-800 {% if(bgColor is defined) %} {{ bgColor }} {% endif %}">delete</span></button>
</form>

